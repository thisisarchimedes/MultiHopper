{
  "extends": "solhint:recommended",
  "rules": {
    "code-complexity": ["error", 8],
    "compiler-version": ["error", ">=0.8.19"],
    "func-name-mixedcase": "off",
    "no-console": "error",  
    "max-states-count": ["warn",20],
    "func-visibility": [
      "error",
      {
        "ignoreConstructors": true
      }
    ],
    "max-line-length": ["error", 140],
    "not-rely-on-time": "off"
  }
}
